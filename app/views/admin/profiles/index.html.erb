<p id="notice"><%= notice %></p>

<h1>Profile 리스트</h1>

<table>
  <thead>
    <tr>
      <th>종류</th>
      <th>Worked at</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= @types[profile.list] %></td>
        <td><%= profile.worked_at %></td>
        <td><%= profile.title %></td>
        <td><%= link_to '수정하기', edit_admin_profile_path(profile) %></td>
        <td><%= link_to '삭제하기', [:admin, profile], method: :delete, data: { confirm: '정말 삭제하겠습니까?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '새 Profile 생성', new_admin_profile_path %>
